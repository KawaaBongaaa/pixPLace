{
  "version": 2,
  "headers": [
    {
      "source": "/api/:match*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://kawaabongaaa.github.io"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With,Content-Type,X-PINGOTHER"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    },
    {
      "source": "/api/auth",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://kawaabongaaa.github.io"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    }
  ],
  "functions": {
    "api/auth.js": {
      "maxDuration": 10
    }
  }
}
